record(bi, "$(P){$(NO)}:conn") {
  field(DTYP, "PSC Ctrl Connected")
  field(INP , "@pscudp$(NO)")
  field(SCAN, "I/O Intr")
  field(ZNAM, "Disconnected")
  field(ONAM, "Connected")
  field(ZSV , "MAJOR")
}

record(bo, "$(P){$(NO)}:reconn") {
  field(DTYP, "PSC Ctrl Reconnect")
  field(OUT , "@pscudp$(NO)")
  field(ZNAM, "Reconnect")
  field(ONAM, "Reconnect")
}


# flush send to socket
record(bo, "$(P){$(NO)}:send") {
  field(DTYP, "PSC Ctrl Send All")
  field(OUT , "@pscudp$(NO)")
  field(ZNAM, "Send")
  field(ONAM, "Send")
}

# writes the UDP packet to the Bipolar Power Converter
record(stringout, "$(P){$(NO)}:speak") {
  field(VAL, "Loop")         # loop is the packet data
  field(DTYP, "PSC Block")
  field(OUT , "@pscudp$(NO) 100")   # 100 is the message number
  field(FLNK, "$(P){$(NO)}:send")
  field(SCAN, "1 second")
}





